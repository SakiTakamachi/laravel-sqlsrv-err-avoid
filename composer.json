{
    "name": "sakiot/laravel-sqlsrv-err-avoid",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SqlsrvErrAvoid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SqlsrvErrAvoid\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
