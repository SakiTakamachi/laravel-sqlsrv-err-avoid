    "authors": [
        {
            "name": "SakiTakamachi",
            "email": "saki@sakiot.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^8.7"
    }
}
